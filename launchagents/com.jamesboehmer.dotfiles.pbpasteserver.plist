<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.jamesboehmer.dotfiles.pbpasteserver</string>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/homebrew/bin/socat</string>
      <string>-U</string>
      <string>tcp-listen:8122,fork,bind=localhost</string>
      <string>EXEC:'pbpaste'</string>
    </array>
    <key>KeepAlive</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
  </dict>
</plist>